# US example

scalar Any

type Hub {
  id: Int!
  country: String!
  name: String!
  address: String!
  state: String!
}

type Query {
  hub(id: Int): [Hub!]!
  schema: Any!
}

input NewHub {
  country: String!
  name: String!
  address: String!
  state: String!
}

type Mutation {
  createHub(input: NewHub!): Hub!
  updateHub(id: Int!, input: NewHub!): Hub
  deleteHub(id: Int!): Boolean
}
