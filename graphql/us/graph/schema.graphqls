# US example

scalar Any

type Hub {
  id: ID!
  country: String!
  name: String!
  address: String!
  state: String!
}

type Query {
  hubs: [Hub!]!
  schema(id: String): Any!
}

input NewHub {
  country: String!
  name: String!
  address: String!
  state: String!
}

type Mutation {
  createHub(input: NewHub!): Hub!
}
